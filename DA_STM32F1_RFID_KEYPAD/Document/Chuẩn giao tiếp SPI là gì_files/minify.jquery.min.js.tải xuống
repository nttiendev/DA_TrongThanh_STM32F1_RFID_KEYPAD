if(wt>ftpt){if(olpb<fbpt){ol.css({top:(wt)})}else{ol.css({top:(fbpt-ol.height())})}
if(orpb<fbpt){or.css({top:(wt)})}else{or.css({top:(fbpt-or.height())})}}else{ol.css({top:(ftpt)});or.css({top:(ftpt)})}}}
